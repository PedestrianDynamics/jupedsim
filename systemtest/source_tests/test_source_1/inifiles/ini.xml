<JuPedSim xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" project="JPS-Project" version="0.8" xsi:noNamespaceSchemaLocation="http://xsd.jupedsim.org/0.6/jps_ini_core.xsd">

  
  <seed>1254</seed>
  
  <geometry>../geometry.xml</geometry>
  <max_sim_time>90</max_sim_time>

  
  <trajectories color_mode="group" format="plain" fps="8">
            <file location="../trajectories/traj.xml" />
  </trajectories>

  
  <logfile>log_sim.txt</logfile>

  
  <traffic_constraints>
  </traffic_constraints>

  <routing>
    <goals>
      <file>../goals.xml</file>
    </goals>
  </routing>

  
  <agents operational_model_id="3">
    <agents_distribution>
      <group agent_parameter_id="1" goal_id="-1" group_id="0" number="0" room_id="0" router_id="1" subroom_id="0" />
      <group agent_parameter_id="1" goal_id="2" group_id="1" number="0" room_id="0" router_id="1" subroom_id="0" />
      <group agent_parameter_id="1" goal_id="1" group_id="2" number="0" room_id="0" router_id="1" subroom_id="0" />
      <group agent_parameter_id="1" goal_id="3" group_id="3" number="0" room_id="0" router_id="1" subroom_id="0" />
      <group agent_parameter_id="1" goal_id="4" group_id="4" number="0" room_id="0" router_id="1" subroom_id="0" />
      <group agent_parameter_id="1" goal_id="-1" group_id="5" number="0" room_id="0" router_id="1" subroom_id="0" x_max="5" x_min="3" y_max="5" y_min="3" />
    </agents_distribution>

    <agents_sources>
      <file>../sources.xml</file>
    </agents_sources>

  </agents>

  <operational_models>
     <model description="Tordeux2015" operational_model_id="3">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.01</stepsize>
        <exit_crossing_strategy>2</exit_crossing_strategy>
        <linkedcells cell_size="2.2" enabled="true" />
        <force_ped D="0.1" a="5" />
        <force_wall D="0.02" a="5" />
      </model_parameters>
      <agent_parameters agent_parameter_id="1">
        <v0 mu="1.0" sigma="0.001" />
        <bmax mu="0.15" sigma="0.00000" /> 
        <bmin mu="0.15" sigma="0.00000" />
        <amin mu="0.15" sigma="0.00000" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.0" sigma="0.00000" />
        <T mu="1" sigma="0.001" />
      </agent_parameters>
    </model>
  </operational_models>

  <route_choice_models>
    <router description="ff_global_shortest" router_id="1">
      <parameters />
    </router>
  </route_choice_models>

</JuPedSim>