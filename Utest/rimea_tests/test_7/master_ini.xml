  <!--persons information and distribution -->
  <agents operational_model_id="[1, 5]">
    <agents_distribution>
       <!-- <30 -->
       <group group_id="0" agent_parameter_id="1"  subroom_id="0" goal_id="-1" number="1

50" router_id="1" pre_movement_mean="0" pre_movement_sigma="0" x_min="60" x_max="80"/>
       <!-- 30-50 -->
       <group group_id="1" agent_parameter_id="2"  subroom_id="0" goal_id="-1" number="150" router_id="1" pre_movement_mean="30" pre_movement_sigma="0" x_min="40" x_max="60"/>
       <!-- >50 -->
       <group group_id="2" agent_parameter_id="3"  subroom_id="0" goal_id="-1" number="150" router_id="1" pre_movement_mean="60" pre_movement_sigma="0" x_min="20" x_max="40"/>
       <!-- mobility -->
      <group group_id="3"  agent_parameter_id="4"  subroom_id="0" goal_id="-1" number="150"  router_id="1"  pre_movement_mean="90" pre_movement_sigma="0" x_min="0" x_max="20"/>
    </agents_distribution>
  </agents>

  <!-- These parameters may be overwritten -->
  <operational_models>
    <model operational_model_id="1" description="gcfm">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.01</stepsize>
        <exit_crossing_strategy>4</exit_crossing_strategy>
        <linkedcells enabled="true" cell_size="2.2" />
        <force_ped nu="0.0" dist_max="3" disteff_max="2" interpolation_width="0.1" />
        <force_wall nu="0.2" dist_max="3" disteff_max="2" interpolation_width="0.1" />
      </model_parameters>
      <agent_parameters agent_parameter_id="1">
        <v0 mu="1.095" sigma="0.2" />
        <v0_upstairs mu="0.5" sigma="0.0" />
        <v0_downstairs mu="0.65" sigma="0.0" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.5" sigma="0.001" />
      </agent_parameters>
      <agent_parameters agent_parameter_id="2">
