<?xml version="1.0" encoding="UTF-8"?>

<JuPedSim project="JPS-Project" version="0.7" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <header>
        <seed>12542</seed>
        <max_sim_time>100</max_sim_time>
        <geometry>bottleneck_geo.xml</geometry>
        <trajectories fps="60" />
    </header>
    <traffic_constraints>
        <doors>
            <door trans_id="2" caption="main_exit" state="open" />
        </doors>
    </traffic_constraints>

    <areas>
        <area id="1">
            <lables>
                <label>exit</label>
            </lables>
            <polygon>
                <vertex px="64.5" py="102.6" />
                <vertex px="64.5" py="101.4" />
                <vertex px="65" py="101.4" />
                <vertex px="65" py="102.6" />
            </polygon>
        </area>
    </areas>
    <agents operational_model_id="3">
        <agents_distribution>
            <group group_id="1" agent_parameter_id="1" room_id="1" subroom_id="0" number="30" goal_id="0" router_id="1" />
        </agents_distribution>
    </agents>
    <operational_models>
        <model operational_model_id="3" description="Tordeux2015">
            <model_parameters>
                <stepsize>0.01</stepsize>
                <exit_crossing_strategy>3</exit_crossing_strategy>
                <linkedcells enabled="true" cell_size="2.2" />
                <force_ped a="8" D="0.1" />
                <force_wall a="5" D="0.02" />
            </model_parameters>
            <agent_parameters agent_parameter_id="1">
                <v0 mu="1.0" sigma="0.001" />
                <bmax mu="0.15" sigma="0.00000" />
                <bmin mu="0.15" sigma="0.00000" />
                <amin mu="0.15" sigma="0.00000" />
                <tau mu="0.5" sigma="0.001" />
                <atau mu="0.0" sigma="0.00000" />
                <T mu="1" sigma="0.001" />
            </agent_parameters>
        </model>
    </operational_models>
    <route_choice_models>
        <router router_id="1" description="global_shortest" />
    </route_choice_models>

</JuPedSim>
